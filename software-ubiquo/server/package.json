{
  "name": "software-ubiquo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:api": "ts-node src/api.ts",
    "dev": "nodemon --exec ts-node src/index.ts",
    "dev:api": "nodemon --exec ts-node src/api.ts",
    "prepare": "husky",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "ts-node src/database/seed.ts",
    "db:populate-brazil": "./scripts/populate-brazil-data.sh",
    "docker:build": "docker build -t ubiqua-backend .",
    "docker:run": "docker run -p 3000:3000 --env-file .env ubiqua-backend",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "docker:restart": "docker compose restart",
    "docker:db:migrate": "docker compose exec backend pnpm run db:migrate",
    "docker:db:seed": "docker compose exec backend pnpm run db:seed",
    "docker:dev:db:migrate": "docker compose -f docker-compose.dev.yml exec backend-dev pnpm run db:migrate",
    "docker:dev:db:seed": "docker compose -f docker-compose.dev.yml exec backend-dev pnpm run db:seed",
    "docker:dev:up": "docker compose -f docker-compose.dev.yml up -d",
    "docker:dev:down": "docker compose -f docker-compose.dev.yml down"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@types/density-clustering": "^1.3.3",
    "@types/express": "^5.0.3",
    "@types/fhir": "^0.0.40",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@types/pg": "^8.15.5",
    "drizzle-kit": "^0.31.5",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "nodemon": "^3.0.2",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "density-clustering": "^1.3.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.6",
    "express": "^5.1.0",
    "fhirpath": "^3.0.0",
    "firebase-admin": "^13.5.0",
    "pg": "^8.16.3"
  }
}
